<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andteam.sep4greenhouse">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.andteam.sep4greenhouse.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.andteam.sep4greenhouse.view.LoginActivity"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.AccountSettingsActivity"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.CreatePlantProfileActivity"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.ModifyAccountActivity"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.ModifyPlantProfileActivity"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.ViewPlantProfileActivity"></activity>
    </application>
</manifest>