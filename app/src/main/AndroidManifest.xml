<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.calcounterapplicaton.myapplication">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".AccountSettingsActivity"></activity>
        <activity android:name=".CreatePlantProfileActivity"></activity>
        <activity android:name=".ModifyAccountActivity"></activity>
        <activity android:name=".ModifyPlantProfileActivity"></activity>
        <activity android:name=".ViewPlantProfileActivity"></activity>
    </application>
</manifest>